---
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:https://www.studentrobotics.org
METHOD:PUBLISH
{% for event in site.events %}
BEGIN:VEVENT
UID:{{ event.url }}
ORGANIZER;CN="Student Robotics":MAILTO:info@studentrobotics.org
GEO:{% reverse_geocode event.geo, geo %}
LOCATION:{% reverse_geocode event.geo, address %}
SUMMARY:{{ event.title }}
CLASS:PUBLIC
DTSTART;VALUE=DATE:{% event_date event ics %}
END:VEVENT
{% endfor %}
END:VCALENDAR
